
public class JavaTest {
	public static void main(String args[]) {
		//Declare variables
		   	int count = 0;
		    double quotient = 1;
		    while(quotient >= 0.0001)
		    {
		            quotient = quotient/2;
		            count++;
		    }
            System.out.println("You divide 1 by 2 " + count + " times");

	}
}

