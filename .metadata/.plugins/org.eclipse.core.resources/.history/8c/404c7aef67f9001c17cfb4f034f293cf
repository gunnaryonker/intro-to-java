import java.util.Scanner;

public class JavaTest {
	public static void main(String args[]) {
		int val = 2;
		switch( val ) {
			case 1: System.out.print("Today ");   
			case 2: System.out.print("Is ");   
			case 3: System.out.print("Really ");
			default: System.out.print("Hot ");
			  break;
		}

	}
}


